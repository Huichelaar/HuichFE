// Silently set destinations to available chapters

// Paralogue 1
CHECK_EVENTID Paralogue1Flag
BEQ 0x0 0x0 0xC
  WM_SETDESTINATION 0x0 0x12 0x0
LABEL 0x0

// Paralogue 2
CHECK_EVENTID Paralogue2Flag
BEQ 0x1 0x0 0xC
  WM_SETDESTINATION 0x0 0x13 0x0
LABEL 0x1

// Paralogue 3
CHECK_EVENTID Paralogue3Flag
BEQ 0x2 0x0 0xC
  WM_SETDESTINATION 0x0 0x14 0x0
LABEL 0x2

// Paralogue 4
CHECK_EVENTID Paralogue4Flag
BEQ 0x3 0x0 0xC
  WM_SETDESTINATION 0x0 0x15 0x0
LABEL 0x3

// Paralogue 5
CHECK_EVENTID Paralogue5Flag
BEQ 0x4 0x0 0xC
  WM_SETDESTINATION 0x0 0x16 0x0
LABEL 0x4

// Chapters 15-18
// Skip if we're called by a main chapter pre-event.
// In that case, they'll be set by our caller.
BNE 0x8 0x0 0x5
  CHECK_EVENTID Act3Flag1
  SADD 0xC3
  CHECK_EVENTID Act3Flag2
  SADD 0xC4

  BNE 0x5 0x0 0x3
    BNE 0x6 0x0 0x4
      // 00, Chapter 15
      WM_SETDESTINATION 0x0 0xE 0x0
      GOTO 0x8

        LABEL 0x5
        BNE 0x7 0x0 0x4
          // 10, Chapter 17
          WM_SETDESTINATION 0x0 0x10 0x0
          GOTO 0x8

            LABEL 0x6
            // 01, Chapter 16
            WM_SETDESTINATION 0x0 0xF 0x0
            GOTO 0x8

              LABEL 0x7
              // 11, Chapter 18
              WM_SETDESTINATION 0x0 0x11 0x0
LABEL 0x8
ENDA